{
  "name": "ontimize-web-ngx-dynamicform-example",
  "version": "4.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "aot": "npm run clean:all && npm run aot:compile && npm run aot:bundle && npm run styles",
    "aot:bundle": "webpack --config config/webpack-aot.config.js --bail",
    "aot:compile": "node_modules/.bin/ngc -p tsconfig.aot.json",
    "start": "ng serve --port 4403",
    "start-aot": "npm run clean:all && npm run aot:compile && ng serve",
    "build": "npm run clean && ng build --base-href=/ontimizeweb/dynamicform/",
    "clean": "rimraf dist",
    "clean:aot": " rimraf src/node_modules && rimraf src/app/**/*.ngfactory.ts && rimraf src/app/**/*.ngsummary.json && rimraf src/app/**/*.ngstyle.ts && rimraf src/app/**/*.d.ts",
    "clean:all": "npm run clean && npm run clean:aot",
    "production": "npm run clean && ng build --aot=false --prod --base-href ./",
    "production-server": "npm run clean && ng build --aot=false --prod --base-href /ontimizeweb/dynamicform/",
    "styles": "node-sass src/styles-aot.scss dist/assets/css/app.css --output-style compressed"
  },
  "private": true,
  "dependencies": {
    "@angular/flex-layout": "6.0.0-beta.18",
