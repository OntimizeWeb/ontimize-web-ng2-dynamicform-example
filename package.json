    "@angular/router": "6.1.10",
    "@angular/upgrade": "6.1.10",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "core-js": "2.5.7",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26",
    "reflect-metadata": "0.1.12",
    "ontimize-web-ngx": "4.0.3",
    "ontimize-web-ngx-dynamicform": "4.0.0",
